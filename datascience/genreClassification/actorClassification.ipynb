    "genre_counts = {}\n",
    "for f in json_files:\n",
    "    data = json.load(open(f))\n",
    "    if type(data[\"cast\"]) == dict:\n",
    "        cast = data[\"cast\"][\"cast\"]\n",
    "    else:\n",
    "        cast = data[\"cast\"]\n",
    "    gc = defaultdict(int)\n",
    "    for x in cast:\n",
    "        for g in x[\"genre_ids\"]:\n",
    "            gc[g] += 1\n",
    "    total = sum(gc.values())\n",
    "    vec = [0] * d\n",
    "    for i, val in gc.items():\n",
    "        vec[genre_names.index(genre_map[i])] += val\n",
    "    for i in range(len(vec)): vec[i] /= total\n",

    "    genre_counts[data[\"_actorID\"]] = {}\n",
